$ var designSystem = data.params.designSystem;
$ var currentComponent = data.params.component;
import util from '../src/common/ds-util';
<lasso-page
    name=`demo-${designSystem}-${currentComponent}`
    cache-key=`demo-${designSystem}-${currentComponent}`
    package-path="./browser.json"
    flags=data.lassoFlags
    dependencies=data.model.dependencies/>
<!DOCTYPE html>
<html class=designSystem>
    <head>
        <meta charset="utf-8"/>
        <link rel="shortcut icon" href="/favicon.ico"/>
        <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
        <meta name="viewport" content="width=device-width"/>
        <title>ebayui-core</title>
        <lasso-head/>
    </head>
    <body>
        <section class="grid">
            <nav class="demo-nav">
                <span class="demo-nav__ds">
                    <for|name, index| of=util.dsList>
                        $ const ds = `ds${name}`;
                        <if (index > 0)>
                            &nbsp;&middot;&nbsp;
                        </if>
                        <${designSystem === ds ? null : "a"} href=`/${ds}/${currentComponent}`>
                            <span>DS${name}</span>
                        </>
                    </for>
                </span>
                <for|component| of=data.model.components>
                    <a href=`/${designSystem}/${component}`>
                        <span>${component}</span>
                    </a>
                </for>
            </nav>
            <main>
                <h1>${data.params.component}</h1>
                <for|example| of=data.model.examples>
                    <div class="example">
                        <h2>${example.name}</h2>
                        <div class="example-code">$!{example.code}</div>
                        <div>
                            <${example.template}/>
                        </div>
                    </div>
                </for>
            </main>
        </section>
        <lasso-body/>
        <script src=process.env.BROWSER_REFRESH_URL/>
    </body>
</html>
